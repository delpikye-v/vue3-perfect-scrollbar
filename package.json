{
  "name": "vue3-perfect-scrollbar-z",
  "version": "1.0.0",
  "description": "Vue3 Scrollbar - Best Wrap for perfect-scrollbar.",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "module": "dist/index.js",
  "unpkg": "dist/index.min.js",
  "module-ems": "dist/index.esm.js",
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "clean": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delpikye-v/vue3-perfect-scrollbar.git"
  },
  "homepage": "https://github.com/delpikye-v/vue3-perfect-scrollbar",
  "author": "Delpi.Kye",
  "keywords": [
    "vue",
    "scrollbar",
    "scrollbar-custom",
    "perfect-scrollbar",
    "ps-scrollbar",
    "vue-scrollbar-custom",
    "vue-scrollbar",
    "vue-perfect-scrollbar",
    "vue3-scrollbar",
    "vue3-perfect-scrollbar"
  ],
  "license": "MIT",
  "dependencies": {
    "perfect-scrollbar": "^1.5.2"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.11",
    "autoprefixer": "^9.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.8",
    "minimist": "^1.2.5",
    "node-sass": "^6.0.1",
    "postcss": "^8.1.9",
    "postcss-cli": "^9.0.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "^6.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass-loader": "^10.1.1",
    "vue": ">=3.0.0"
  },
  "engines": {
    "node": ">=12"
  }
}
